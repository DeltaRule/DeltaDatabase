      "title": "HTTP Error Rate (4xx / 5xx)",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by(status_code) (rate(deltadatabase_main_http_requests_total{status_code=~\"4..|5..\"}[1m]))",
          "legendFormat": "{{status_code}}"
        }
      ]
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 26 },
