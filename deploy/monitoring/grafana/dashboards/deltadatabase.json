        "defaults": { "unit": "short", "color": { "mode": "palette-classic" }, "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "orange", "value": 0 }] } },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 27 },
      "id": 7,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "auto", "colorMode": "value" },
