      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by(operation, status) (rate(deltadatabase_main_process_requests_total[1m]))",
          "legendFormat": "{{operation}} {{status}}"
        }
      ]
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "s", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "id": 4,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list" } },
      "title": "Process Latency (p95)",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "histogram_quantile(0.95, sum by(le, operation) (rate(deltadatabase_main_process_request_duration_seconds_bucket[1m])))",
          "legendFormat": "{{operation}} p95"
        }
      ]
