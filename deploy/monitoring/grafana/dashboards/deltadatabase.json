{
  "__inputs": [],
  "__requires": [],
  "annotations": { "list": [] },
  "description": "Full observability dashboard for DeltaDatabase — Main Worker and Processing Workers.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Main Worker — gRPC",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "id": 1,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list" } },
      "title": "Subscribe RPS",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by(status) (rate(deltadatabase_main_subscribe_requests_total[1m]))",
          "legendFormat": "{{status}}"
        }
      ]
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "s", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "id": 2,
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list" } },
      "title": "Subscribe Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "histogram_quantile(0.50, sum by(le) (rate(deltadatabase_main_subscribe_request_duration_seconds_bucket[1m])))",
          "legendFormat": "p50"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "histogram_quantile(0.95, sum by(le) (rate(deltadatabase_main_subscribe_request_duration_seconds_bucket[1m])))",
          "legendFormat": "p95"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "histogram_quantile(0.99, sum by(le) (rate(deltadatabase_main_subscribe_request_duration_seconds_bucket[1m])))",
          "legendFormat": "p99"
        }
      ]
    },

    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "color": { "mode": "palette-classic" } },
        "overrides": []
